{
    "name":         "jms/i18n-routing-bundle",
    "description":  "This bundle allows you to create i18n routes.",
    "keywords":     ["translation","multilanguage","routing"],
    "homepage": "http://jmsyst.com/bundles/JMSI18nRoutingBundle",       
    "type":         "symfony-bundle",
    "license":      "Apache2",
    "authors": [
        {
            "name":   "Johannes M. Schmitt",
            "email":  "schmittjoh@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": "~2.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "<2.2.12",
        "symfony/css-selector": "<2.2.12",
        "symfony/http-kernel": "<2.2.12",
        "symfony/validator": "<2.2.12",
        "symfony/browser-kit": "<2.2.12",
        "symfony/finder": "<2.2.12",
        "symfony/form": "<2.2.12",
        "symfony/yaml": "<2.2.12",
        "symfony/process": "<2.2.12",
        "symfony/routing": "<2.2.12",
        "symfony/http-foundation": "<2.2.12",
        "symfony/class-loader": "<2.2.12",
        "symfony/twig-bundle": "<2.2.12",
        "sensio/framework-extra-bundle": "<2.2.12",
        "jms/di-extra-bundle": "*"
    },
    "suggest": {
        "jms/translation-bundle": "If you want to use the RouteTranslation extractor"
    },
    "autoload": {
        "psr-0": { "JMS\\I18nRoutingBundle": "" }
    },
    "target-dir": "JMS/I18nRoutingBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
